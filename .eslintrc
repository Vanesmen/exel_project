{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "operator-linebreak": ["error", "before"],
    "arrow-parens": ["error", "as-needed"],
    "linebreak-style": ["error", "windows"],
    "no-debugger": "off",
    "no-unused-vars": "off"
  },
  "extends": ["eslint:recommended", "google"]
}